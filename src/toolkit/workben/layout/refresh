  && cp ../../${INPATH}/lib/*.so ../../../solver/300/${INPATH}/lib \
  && cp -f ../..$INPATH/lib/libtkx.so $OOO_INSTALL_PREFIX/openoffice.org/basis3.0/program \
  && dmake debug=true \
  && ../../${INPATH}/bin/test "$@"
