+    <xsl:variable name="localname" select="@localname"/>
+    <xsl:if test="generate-id(.) = generate-id(key('same-token-name', $localname)[1])">
+      <xsl:text>
+    case </xsl:text>    
+    <xsl:call-template name="fastlocalname"/>
+    <xsl:text>:
+        sResult +=  "</xsl:text>
+        <xsl:value-of select="$localname"/>
+        <xsl:text>";
+        break;</xsl:text>        
+    </xsl:if>
+  </xsl:for-each>
+<xsl:text>
+    }
+
+    return sResult;
