+    </xsl:call-template>
+    <xsl:text>
+      nResult = Sprm::PARAGRAPH;
+      break;</xsl:text>
+    <xsl:call-template name="sprmkindcase">
+      <xsl:with-param name="kind">character</xsl:with-param>
+    </xsl:call-template>
+    <xsl:text>
+      nResult = Sprm::CHARACTER;
+      break;</xsl:text>
+    <xsl:call-template name="sprmkindcase">
+      <xsl:with-param name="kind">table</xsl:with-param>
+    </xsl:call-template>
+    <xsl:text>
+      nResult = Sprm::TABLE;
+      break;</xsl:text>
+    <xsl:text>
+    default:
+      break;
+    }
+
+    return nResult;
+}</xsl:text>
+  </xsl:template>
