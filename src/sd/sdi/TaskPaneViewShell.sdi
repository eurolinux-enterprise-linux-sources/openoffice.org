	]
	SID_COPY
	[
		ExecMethod = Execute ;
		StateMethod = GetState ;
	]
	SID_PASTE
	[
		ExecMethod = Execute ;
		StateMethod = GetState ;
	]
    SID_DELETE_MASTER_PAGE
	[
		ExecMethod = Execute ;
		StateMethod = GetState ;
	]
}

